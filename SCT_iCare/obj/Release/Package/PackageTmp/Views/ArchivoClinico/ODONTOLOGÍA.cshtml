                </div>

                <script type="text/javascript">
                    function f_Diente_18() {
                        chk_car_check_Diente_18 = document.getElementById("s_Diente_18");
                        chk_ret_check_Diente_18 = document.getElementById("ret_Diente_18");
                        chk_obt_check_Diente_18 = document.getElementById("obt_Diente_18");

                        if (chk_car_check_Diente_18.checked) {
                            chk_ret_check_Diente_18.checked = false;
                            chk_obt_check_Diente_18.checked = false;
                            document.getElementById("text_cDiente_18").value = chk_car_check_Diente_18.value;
                            document.getElementById("text_rDiente_18").value = null;
                            document.getElementById("text_oDiente_18").value = null;
                        }
                        else {
                            document.getElementById("text_cDiente_18").value = null;
                        }
                    }
                </script>

                <script type="text/javascript">
                    function un_ret_Diente_18() {
                        unr_check_Diente_18 = document.getElementById("s_Diente_18");
                        unr_ret_check_Diente_18 = document.getElementById("ret_Diente_18");
                        unr_obt_check_Diente_18 = document.getElementById("obt_Diente_18");

                        if (unr_ret_check_Diente_18.checked) {

                            unr_check_Diente_18.checked = false;
                            unr_obt_check_Diente_18.checked = false;
                            document.getElementById("text_rDiente_18").value = unr_ret_check_Diente_18.value;
                            document.getElementById("text_cDiente_18").value = null;
                            document.getElementById("text_oDiente_18").value = null;
                      
                        }
                        else {
                            document.getElementById("text_rDiente_18").value = null;
                        }

                    }
                </script>

                <script type="text/javascript">
                    function un_obt_Diente_18() {
                        uno_check_Diente_18 = document.getElementById("s_Diente_18");
                        uno_ret_check_Diente_18 = document.getElementById("ret_Diente_18");
                        uno_obt_check_Diente_18 = document.getElementById("obt_Diente_18");
                       
